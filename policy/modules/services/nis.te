role ypbind_roles types ypbind_t;

type ypbind_initrc_exec_t;
init_script_file(ypbind_initrc_exec_t)

type ypbind_runtime_t alias ypbind_var_run_t;
files_pid_file(ypbind_runtime_t)

type ypbind_tmp_t;
