/etc/openca(/.*)?	gen_system_context(openca_etc_t)
/etc/openca/.*\.in(/.*)?	gen_system_context(openca_etc_in_t)
/etc/openca/rbac(/.*)?	gen_system_context(openca_etc_writeable_t)

/usr/share/openca(/.*)?	gen_system_context(openca_usr_share_t)
/usr/share/openca/cgi-bin/ca/.+	--	gen_system_context(openca_ca_exec_t)

/var/lib/openca(/.*)?	gen_system_context(openca_var_lib_t)
/var/lib/openca/crypto/keys(/.*)?	gen_system_context(openca_var_lib_keys_t)
