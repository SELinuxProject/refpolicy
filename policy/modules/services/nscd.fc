/etc/rc\.d/init\.d/nscd	--	gen_system_context(nscd_initrc_exec_t)

/usr/bin/nscd	--	gen_system_context(nscd_exec_t)

/usr/sbin/nscd	--	gen_system_context(nscd_exec_t)

/var/cache/nscd(/.*)?	gen_system_context(nscd_runtime_t)

/var/db/nscd(/.*)?	gen_system_context(nscd_runtime_t)

/var/log/nscd\.log.*	--	gen_system_context(nscd_log_t)

/run/nscd(/.*)?	gen_system_context(nscd_runtime_t)
/run/nscd\.pid	--	gen_system_context(nscd_runtime_t)
/run/\.nscd_socket	-s	gen_system_context(nscd_runtime_t)
