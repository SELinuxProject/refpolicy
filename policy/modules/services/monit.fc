/etc/rc\.d/init\.d/monit	--	gen_system_context(monit_initrc_exec_t)

/etc/monit(/.*)?			gen_system_context(monit_conf_t)

/run/monit\.pid			--	gen_system_context(monit_runtime_t)
/run/monit\.socket		-s	gen_system_context(monit_runtime_t)

/usr/bin/monit			--	gen_system_context(monit_exec_t)

/usr/lib/systemd/system/monit.*	--	gen_system_context(monit_unit_t)

/var/lib/monit(/.*)?			gen_system_context(monit_var_lib_t)

/var/log/monit\.log.*		--	gen_system_context(monit_log_t)
