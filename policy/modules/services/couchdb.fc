/etc/couchdb(/.*)?			gen_system_context(couchdb_conf_t)
/etc/rc\.d/init\.d/couchdb	--	gen_system_context(couchdb_initrc_exec_t)

/usr/bin/couchdb		--	gen_system_context(couchdb_exec_t)
/usr/lib/couchdb/bin/couchjs	--	gen_system_context(couchdb_js_exec_t)

/var/lib/couchdb(/.*)?			gen_system_context(couchdb_var_lib_t)
/var/log/couchdb(/.*)?			gen_system_context(couchdb_log_t)
/run/couchdb(/.*)?			gen_system_context(couchdb_runtime_t)
