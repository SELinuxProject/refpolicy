/usr/sbin/bconsole	--	gen_system_context(bacula_admin_exec_t)

/var/lib/bacula.*	gen_system_context(bacula_var_lib_t)

/var/log/bacula.*	gen_system_context(bacula_log_t)

/run/bacula.*	--	gen_system_context(bacula_runtime_t)

/var/spool/bacula.*	gen_system_context(bacula_spool_t)
