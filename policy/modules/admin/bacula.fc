/bacula(/.*)?	gen_system_context(bacula_store_t)

/etc/bacula.*	gen_system_context(bacula_etc_t)

/etc/rc\.d/init\.d/bacula.*	--	gen_system_context(bacula_initrc_exec_t)

/usr/bin/bacula.*	--	gen_system_context(bacula_exec_t)
/usr/bin/bat	--	gen_system_context(bacula_admin_exec_t)
