selinux_domtrans_loadpol(rpm_script_t)
selinux_domtrans_restorecon(rpm_script_t)

userdom_use_all_user_fd(rpm_script_t)

optional_policy(`bootloader.te', `
bootloader_domtrans(rpm_script_t)
')

